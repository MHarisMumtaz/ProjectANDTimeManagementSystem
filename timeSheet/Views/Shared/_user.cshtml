<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>TimeSheet System</title>
    <link type="text/css" href="@Url.Content("~/bootstrap/css/bootstrap.min.css")" rel="stylesheet">
    <link type="text/css" href="@Url.Content("~/bootstrap/css/bootstrap-responsive.min.css")" rel="stylesheet">

    <link type="text/css" href="@Url.Content("~/css/theme.css")" rel="stylesheet">

    <link type="text/css" href="@Url.Content("~/images/icons/css/font-awesome.css")" rel="stylesheet">

    <link type="text/css" href='@Url.Content("http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,600")'
          rel='stylesheet'>

    @RenderSection("header", false)

</head>
<body>
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".navbar-inverse-collapse">
                    <i class="icon-reorder shaded"></i>
                </a><a class="brand" href="#">TimeSheet System </a>
                <div class="nav-collapse collapse navbar-inverse-collapse">

                    <ul class="nav pull-right">
                        <li class="nav-user dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">

                             @timeSheet.Sessions.UserID
                                <b class="caret"></b>
                            </a>
                            <ul class="dropdown-menu">
                              
                                <li class="divider"></li>
                                <li><a href="@Url.Action("Login", "Logins")">Logout</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <!-- /.nav-collapse -->
            </div>
        </div>
        <!-- /navbar-inner -->
    </div>
    <!-- /navbar -->
    <div class="wrapper">
        <div class="container">
            <div class="row">
                <div class="span3">
                    <div class="sidebar">
                        <ul class="widget widget-menu unstyled">
                            <li class="active">
                                <a href="@Url.Action("admin","Admins")">
                                    <i class="menu-icon icon-dashboard"></i>Dashboard
                                </a>
                            </li>
                            <li>


                                <a href="@Url.Action("CreateUser", "Admins")">
                                    <i class="menu-icon icon-tasks"></i>Create User
                                </a>
                            </li>
                           
                                <li class="active">
                                    <a href="@Url.Action("ProjectCreation", "Admins")">
                                        <i class="menu-icon icon-dashboard"></i>Create Project
                                    </a>
                                </li>
                           
                           
                            <li class="active">
                                <a href="@Url.Action("ProjectAsign", "Admins")">
                                    <i class="menu-icon icon-dashboard"></i>Project Assignment
                                </a>
                            </li>
                        </ul>

                        
                    </div>
                    <!--/.sidebar-->
                </div>
                <!--/.span3-->
                <div class="span9">
                    <div class="content">
                        @RenderBody()
                    </div>
                    <!--/.content-->
                </div>
                <!--/.span9-->
            </div>
        </div>
        <!--/.container-->
    </div>
    <!--/.wrapper-->
    <div class="footer">
        <div class="container">
            <b class="copyright">&copy; Trailblazer 2015 </b>All rights reserved.
        </div>
    </div>

    <script src="@Url.Content("~/scripts/jquery-1.9.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/jquery-ui-1.10.1.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/flot/jquery.flot.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/flot/jquery.flot.resize.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/datatables/jquery.dataTables.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/common.js")" type="text/javascript"></script>
    @RenderSection("footer", false)

</body>
</html>
